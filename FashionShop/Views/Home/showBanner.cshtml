@model IEnumerable<FashionShop.Models.EF.Category>


<!-- block1 -->
@foreach (var item in Model)
{
    <div class="col-sm-10 col-md-8 col-lg-4 m-l-r-auto">
        <div class="block1 hov-img-zoom pos-relative m-b-30" style="border-radius: 20px">
            <img src="~/Images/CategoryImg/@item.CateImg" alt="IMG-BENNER">

            <div class="block1-wrapbtn w-size2">
                <!-- Button -->
                <a href="/@item.Meta/tat-ca-san-pham" class="flex-c-m size2 m-text2 bg3 hov1 trans-0-4">
                    @item.CategoryName
                </a>
            </div>
        </div>
    </div>
}


    




